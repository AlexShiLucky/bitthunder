        depends on STM32_PB0_MODE_INPUT

    config STM32_PB0_CONF_OUTPUT_PP
        bool "Output - push-pull"
        depends on !STM32_PB0_MODE_INPUT

    config STM32_PB0_CONF_OUTPUT_OD
        bool "Output - open-drain"
        depends on !STM32_PB0_MODE_INPUT

endchoice
