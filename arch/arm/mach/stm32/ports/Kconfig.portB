#
#   PB0
#
menu "PB0"

choice STM32_PB0_FUNCTION
    prompt "Function"
    default STM32_PB0_GPIO

    config STM32_PB0_GPIO
        bool "GPIO"

    config STM32_PB0_AF
        bool "Alternate Function (peripheral)"
        ---help---
        Select this if you want to use a peripheral signal with this pin.
        You must further correctly configure the pin

endchoice

choice STM32_PB0_MODE
    prompt "PB0 Mode"
    default STM32_PB0_MODE_INPUT

    config STM32_PB0_MODE_INPUT
        bool "Input"

    config STM32_PB0_MODE_OUTPUT_2MHz
        bool "Output -  2MHz"

    config STM32_PB0_MODE_OUTPUT_10MHz
        bool "Output - 10MHz"

    config STM32_PB0_MODE_OUTPUT_50MHz
        bool "Output - 50MHz"

endchoice

choice STM32_PB0_CONF
    prompt "PB0 Config"
    default STM32_PB0_CONF_INPUT_FLOATING if STM32_PB0_MODE_INPUT
    default STM32_PB0_CONF_OUTPUT_PP if !STM32_PB0_MODE_INPUT

