# BT_CONFIG_SYSLOG_REMOVE_PRINTK is not set

#
# Library Support
#
BT_CONFIG_LIB_PRINTF=y

#
# printf features
#
BT_CONFIG_LIB_PRINTF_SUPPORT_HEX=y

#
# Drivers
#

#
# Configure all required driver sub-systems
#
# BT_CONFIG_DRIVERS_SDCARD is not set

#
# Kernel Options
#
BT_CONFIG_KERNEL_FREERTOS=y
# BT_CONFIG_KERNEL_BT is not set

#
# FreeRTOS Specific options
#
BT_CONFIG_KERNEL_TICK_RATE=1000
