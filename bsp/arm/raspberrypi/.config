#
# GPIOs
#
BT_CONFIG_MAX_GPIO_CONTROLLERS=1

#
# Interrupts
#
BT_CONFIG_MAX_INTERRUPT_CONTROLLERS=1
BT_CONFIG_INTERRUPTS_SOFTIRQ=y
BT_CONFIG_INTERRUPTS_SOFTIRQ_PRIORITY=0
BT_CONFIG_INTERRUPTS_SOFTIRQ_MAX=4

#
# Tasklets
#
BT_CONFIG_TASKLETS=y

#
# Syslog
#
# BT_CONFIG_SYSLOG_REMOVE_PRINTK is not set

#
# Library Support
#
BT_CONFIG_LIB_PRINTF=y

#
# printf features
#
BT_CONFIG_LIB_PRINTF_SUPPORT_HEX=y

#
# Drivers
#

#
# Configure all required driver sub-systems
#
BT_CONFIG_DRIVERS_SDCARD=y

#
# SDCard Subsystem
