BT_CONFIG_HOST_LINUX=y
# BT_CONFIG_HOST_WINDOWS is not set
# BT_CONFIG_HOST_OTHER is not set
BT_CONFIG_TOOLCHAIN="/opt/codesourcery/bin/arm-none-eabi-"

#
# Build Configuration
#

#
# System Architecture
#
BT_CONFIG_ARCH_ARM=y
# BT_CONFIG_ARCH_PPC is not set
BT_CONFIG_ARCH="arm"

#
# ARM Options
