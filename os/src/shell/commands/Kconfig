menu "Commands"

config SHELL_CMD_ATAGS
	bool "ATAG Support"
	depends on SHELL
	default n

if SHELL_CMD_ATAGS
source os/src/shell/commands/atag/Kconfig
endif

config SHELL_CMD_BOOT
    bool "boot"
	depends on SHELL
	default n

config SHELL_CMD_BOOT_JTAG
    bool "boot_jtag"
	depends on SHELL
	default n

config SHELL_CMD_CAT
	bool "cat"
	depends on SHELL
	default n
	select FILE

config SHELL_CMD_CD
    bool "cd"
	depends on SHELL
	depends on PROCESS_CWD
	default n
	select FILE

config SHELL_CMD_CP
    bool "cp"
	depends on SHELL
	default n
	select FILE

config SHELL_CMD_CP_BUFFER_SIZE
	int "cp buffer size"
	depends on SHELL_CMD_CP
	default 4096

