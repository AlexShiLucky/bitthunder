#
# BCM2835 Platform Options
#
BT_CONFIG_MACH_BCM2835_USE_STARTUP=y
BT_CONFIG_MACH_BCM2835_SYSCLOCK_FREQ=250000000

#
# Peripheral Selection
#
BT_CONFIG_MACH_BCM2835_BOOTLOG_NULL=y
BT_CONFIG_MACH_BCM2835_TOTAL_IRQ=72
BT_CONFIG_SUBARCH="bcm2835"

#
# BSP Setup
#
BT_CONFIG_BSP_ARM_RASPBERRY_PI=y
BT_CONFIG_BSP_DIR="bsp/arm/raspberrypi/"
